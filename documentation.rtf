\par }\pard\plain \ltrpar\s3\ql \li0\ri0\sb240\sa60\keepn\widctlpar\wrapdefault\aspalpha\aspnum\faauto\outlinelevel2\adjustright\rin0\lin0\itap0\pararsid860613 \rtlch\fcs1 \ab\af1\afs26\alang1025 \ltrch\fcs0 
